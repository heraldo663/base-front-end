{
  "name": "baseProject",
  "version": "1.0.0",
  "description": "Basic front end Setup",
  "main": "js/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:scss": "node-sass -w src/scss/style.scss src/css/pre.css",
    "watch:postcss": "postcss src/css/pre.css -o dist/css/style.css -w",
    "dev:css": "concurrently \"npm run watch:scss arg\" \"npm run watch:postcss\"",
    "dev:js": "babel src/js/main.js --watch --out-file dist/js/bundle.js"
  },
  "author": "Heraldo Ecard Roque Filho",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "autoprefixer": "^9.3.1",
    "babel-preset-minify": "^0.5.0",
    "concurrently": "^4.1.0",
    "cssnano": "^4.1.7",
    "node-sass": "^4.10.0",
    "postcss-cli": "^6.0.1"
  },
  "dependencies": {}
}